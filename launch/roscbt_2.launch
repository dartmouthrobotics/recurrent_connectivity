<?xml version="1.0"?>
<launch>
    <param name="use_sim_time" value="true" />
    <group ns="robot_0">
        <node pkg="nav2d_karto" name="Mapper" type="mapper">
            <remap from="karto_out" to="/karto_out" />
        </node>
    </group>
    <group ns="robot_1">
        <node pkg="nav2d_karto" name="Mapper" type="mapper">
            <remap from="karto_out" to="/karto_out" />
        </node>
    </group>

    <group ns="robot_2">
        <node pkg="nav2d_karto" name="Mapper" type="mapper">
            <remap from="karto_out" to="/karto_out" />
        </node>
    </group>

    <node name="roscbt" pkg="recurrent_connectivity" type="roscbt.py" respawn="true">
        <rosparam file="$(find recurrent_connectivity)/param/roscbt_config2.yaml"/>
    </node>

</launch>
